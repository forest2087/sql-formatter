{
    "name": "4st/sql-formatter",
    "version": "2.0.0",
    "description": "a PHP SQL highlighting library",
    "homepage": "https://github.com/forest2087/sql-formatter",
    "keywords": ["sql", "highlight"],
    "minimum-stability": "dev",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "illuminate/support": ">=5.0.0"
    },
    "authors": [
        {
            "name": "Jeremy Dorn",
            "email": "jeremy@jeremydorn.com",
            "homepage": "http://jeremydorn.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "FW\\SqlFormatter\\": "lib/"
        },
        "classmap": ["lib"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "bin": ["bin/sql-formatter"]
}
